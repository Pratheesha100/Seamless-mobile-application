            android:layout_height="685dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="3dp"
            android:layout_marginTop="96dp"
            android:background="@color/primary1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/back_navigation">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center">

            <!-- First Row of Categories -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:orientation="horizontal"
                android:gravity="center">

                <!-- First Category -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/category_image_1"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:contentDescription="@string/description"
                        android:src="@drawable/hanger" />

                    <TextView
                        android:id="@+id/hanger"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/clothes"
                        android:fontFamily="sans-serif-medium"
                        android:textSize="18sp"
                        android:textColor="@android:color/black"
                        android:layout_marginTop="8dp" />
                </LinearLayout>
                <!-- Second Category -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_marginTop="11dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/category_image_2"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:contentDescription="Category 2"
                        android:src="@drawable/shoes" />

                    <TextView
                        android:id="@+id/category_name_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/shoes"
                        android:fontFamily="sans-serif-medium"
                        android:textSize="18sp"
                        android:textColor="@android:color/black"
                        android:layout_marginTop="8dp" />
                </LinearLayout>
            </LinearLayout>


                <!-- First Row of Categories -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <!-- First Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="11dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image3"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="@string/description"
                            android:src="@drawable/necklace" />

                        <TextView
                            android:id="@+id/category_name_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/jwe"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="8dp" />
                    </LinearLayout>
                    <!-- Second Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_marginTop="11dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_4"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="Category 2"
                            android:src="@drawable/beauty" />

                        <TextView
                            android:id="@+id/category_name_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="sans-serif-medium"
                            android:text="@string/beauty"
                            android:textColor="@android:color/black"
                            android:textSize="18sp" />
                    </LinearLayout>
                </LinearLayout>


                <!-- First Row of Categories -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <!-- First Category -->
                    <LinearLayout
