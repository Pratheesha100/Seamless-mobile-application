                        android:layout_height="wrap_content"
                        android:layout_marginTop="11dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_5"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="@string/description"
                            android:src="@drawable/travelbag" />

                        <TextView
                            android:id="@+id/category_name_5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/bag"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="8dp" />
                    </LinearLayout>
                    <!-- Second Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_marginTop="11dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_6"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="Category 2"
                            android:src="@drawable/hair" />

                        <TextView
                            android:id="@+id/category_name_6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="sans-serif-medium"
                            android:text="@string/hair"
                            android:textColor="@android:color/black"
                            android:textSize="18sp" />
                    </LinearLayout>
                </LinearLayout>

                <!-- First Row of Categories -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <!-- First Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="11dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_7"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="@string/description"
                            android:src="@drawable/tools" />

                        <TextView
                            android:id="@+id/category_name_7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tools"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="8dp" />
                    </LinearLayout>
                    <!-- Second Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_marginTop="11dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_8"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="Category 2"
                            android:src="@drawable/computer" />

                        <TextView
                            android:id="@+id/category_name_8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/computer"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="8dp" />
                    </LinearLayout>
                </LinearLayout>

                <!-- First Row of Categories -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:orientation="horizontal"
                    android:gravity="center" >

                    <!-- First Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_9"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="@string/description"
                            android:src="@drawable/kids" />

                        <TextView
                            android:id="@+id/category_name_9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/kids"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="-1dp" />
                    </LinearLayout>
                    <!-- Second Category -->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_marginTop="1dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/category_image_10"
                            android:layout_width="110dp"
                            android:layout_height="110dp"
                            android:contentDescription="Category 2"
                            android:src="@drawable/chest" />

                        <TextView
                            android:id="@+id/category_name_10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/furniture"
                            android:fontFamily="sans-serif-medium"
                            android:textSize="18sp"
                            android:textColor="@android:color/black"
                            android:layout_marginTop="-1dp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>